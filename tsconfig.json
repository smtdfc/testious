{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "esModuleInterop": true,
    "sourceMap": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "declaration": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "paths": {
      "testious-*": ["./packages/*/dist/index.d.ts"]
    }
  }
}
