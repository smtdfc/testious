{
	"name": "testious",
	"version": "0.0.1",
	"description": "Simple testing framework",
	"homepage": "https://github.com/smtdfc/testious#readme",
	"bugs": {
		"url": "https://github.com/smtdfc/testious/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/smtdfc/testious.git"
	},
	"license": "MIT",
	"author": "smtdfc",
	"type": "module",
	"scripts": {
		"dec": "tsc --declaration --allowJs --emitDeclarationOnly",
		"type": "tsc --noEmit",
		"build": "NODE_ENV=production rollup -c --environment MINIFY:true",
		"build:dev": "NODE_ENV=development rollup -c",
		"start": "NODE_ENV=production node dist/index.js",
		"dev": "NODE_ENV=development node dist/index.js",
		"test": "echo \"No tests yet\"",
		"prepublishOnly": "npm run dec && npm run build"
	},
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/index.js",
			"default": "./dist/index.js"
		}
	},
	"files": [
		"dist/**"
	]
}